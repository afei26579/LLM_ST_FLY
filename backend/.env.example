# .env.example - 环境变量模板
# 复制为 .env.development 或 .env.production 后填写真实值

DJANGO_ENV=development # development or production

# 安全密钥（使用命令生成）
SECRET_KEY=your-generated-secret-key-here

# 调试模式
DEBUG=True # True or False

# 允许的主机（用逗号分隔）
ALLOWED_HOSTS=localhost,127.0.0.1 # 或者你的服务器 IP，域名

# 数据库密码
PSQL_PWD=your_postgres_password

# API 密钥
DASHSCOPE_API_KEY=your_dashscope_api_key # DashScope API KEY

# 邮件密码（QQ邮箱授权码）
EMAIL_HOST_PASSWORD=your_qq_app_password # 如果没有授权码请忽略此行

# 前端地址
FRONTEND_URL=http://localhost:5173 # 如果你部署在子目录，请加上子目录名，例如 http://localhost/dashboard/